{
  "name": "feed-v0",
  "version": "1.0.0",
  "description": "Mono repo of the feed project.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ganache": "bash ./scripts/ganache.sh",
    "init": "node ./helpers/Init.js",
    "truffle-compile": "truffle compile --config truffle-config.cjs",
    "contract-size": "truffle run contract-size --config truffle-config.cjs",
    "move-contracts": "node ./helpers/MoveContracts",
    "compile-contracts": "run-s truffle-compile move-contracts",
    "web3-engine-test": "node ./web3-engine/test/Web3EngineTest.js",
    "helper-test": "node ./web3-engine/test/HelperTest.js",
    "user-register-test": "node ./contracts/dapp/user-register/test/UserRegisterTest.js",
    "messages-test": "node ./contracts/dapp/messages/test/MessagesTest.js",
    "feed-test": "node ./contracts/dapp/feed/test/FeedTest.js",
    "wallet-test": "node ./test/WalletTest.js",
    "cid-testing": "node ./cid-testing/test.js",
    "install-web3-engine": "npm install ./web3-engine/feed-web3-engine-1.0.0.tgz",
    "pack-engine": "cd web3-engine && npm pack",
    "install-webpage-engine": "cd webpage && npm run install-web3-engine",
    "init-feed": "run-s init pack-engine install-webpage-engine install-web3-engine",
    "feed-webpage": "cd webpage && npm run start",
    "webrtc-star": "webrtc-star --host=127.0.0.1 --port 13579"
  },
  "author": "STB",
  "license": "ISC",
  "dependencies": {
    "@feed/web3-engine": "file:web3-engine/feed-web3-engine-1.0.0.tgz",
    "@openzeppelin/contracts": "^4.7.3",
    "@ratio-labs/web3-engine": "file:web3-engine/ratio-labs-web3-engine-1.0.0.tgz",
    "@truffle/contract": "^4.6.2",
    "assert": "^2.0.0",
    "bip39": "^3.0.4",
    "crypto-js": "^4.1.1",
    "eccrypto": "^1.1.6",
    "ethers": "^5.7.1",
    "file-type": "^18.0.0",
    "ganache": "^7.4.1",
    "hdkey": "^2.0.1",
    "ipfs": "^0.63.5",
    "it-all": "^1.0.6",
    "it-to-buffer": "^3.0.0",
    "libp2p-webrtc-star-signalling-server": "^0.1.2",
    "mime-kind": "^4.0.0",
    "multiformats": "^9.7.1",
    "npm-run-all": "^4.1.5",
    "runes2": "^1.0.7",
    "truffle": "^5.5.26",
    "truffle-contract-size": "^2.0.1",
    "uint8arrays": "^3.1.1",
    "vrf-solidity": "^0.2.3",
    "web3": "^1.7.5"
  },
  "devDependencies": {
    "@types/eccrypto": "^1.1.3"
  }
}
